parserImport XOCL;
parserImport Parser::BNF;

import OCL;

context Env::Env

  @Grammar extends OCL::OCL.grammar
  
    Env ::= bs = Bs {
    
      bs->iterate(pair e = [| Env::NullEnv() |] |
        [| <e>.bind(<StrExp(pair->head)>,<pair->tail>) |])
        
    }.
    
    Bs ::= b = B bs = (',' B)* { Seq{b|bs} } | { Seq{} }.
    
    B ::= n = Name '=' e = LogicalExp { Seq{n|e} }.
    
  end